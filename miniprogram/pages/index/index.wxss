.todo-item {
  background-color: #fff;
  border-radius: 25px;
  padding: 16px !important;
  margin: 16px !important;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16), 0px 3px 6px rgba(0, 0, 0, 0.23);
}
.toto-item-title {
  color: rgba(0, 0, 0, 0.88);
  font-weight: 600;
  font-size: 16px;
}
.green {
  color: green;
}
.red {
  color: red;
}
.around {
  display: flex;
  justify-content: space-around;
}
